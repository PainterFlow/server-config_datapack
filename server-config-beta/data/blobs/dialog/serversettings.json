{
    "type": "minecraft:multi_action",
    "title": "Server Settings",
    "external_title": "blob?",
    "actions": [
        {
            "label": "Gamerules",
            "width": 100,
            "action": {
                "type": "run_command",
                "command": "function blobs:settings/display/display_gamerules with storage settings settings.gamerules[]"
            }
        },
        {
            "label": "Quick Actions",
            "width": 100,
            "action": {
                "type": "show_dialog",
                "dialog": "blobs:quick_actions"
            },
            "tooltip": "Execute Quick Commands"
        },
        {
            "label": "Reload Server",
            "width": 100,
            "action": {
                "type": "run_command",
                "command": "function blobs:reload"
            }
        },
        {
            "label": "AFK",
            "width": 100,
            "action": {
                "type": "show_dialog",
                "dialog": "blobs:beta"
            }
        }
    ],
    "columns": 2,
    "pause": false,
    "can_close_with_escape": true,
    "after_action": "none"

}